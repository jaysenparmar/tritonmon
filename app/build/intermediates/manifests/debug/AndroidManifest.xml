            android:label="@string/title_activity_users_pokemon" >
        </activity>
        <activity
            android:name="tritonmon.mobile.com.tritonmon.Welcome"
            android:label="@string/title_activity_welcome" >
        </activity>
        <activity
            android:name="tritonmon.mobile.com.tritonmon.CreateAvatar"
            android:label="@string/title_activity_create_avatar" >
        </activity>
        <activity
            android:name="tritonmon.mobile.com.tritonmon.PokeCenter"
            android:label="@string/title_activity_poke_center" >
